- if !dependencies.nil? && !dependencies.empty?
  - deps = dependencies.map { |dep| all_proposals["#{dep["barclamp"]}_#{dep["inst"]}"] }.select { |p| p.real_status(@active) != 'ready' }
  - if !deps.empty?
    %div
      = t(".waiting_for_proposals", :props => deps.map { |p| "#{p.barclamp} #{p.name}" }.to_sentence)
